;; Cortado init file example
;; This would normally go in ~/.cortadorc

(print "Loading Cortado init file...")

;; Define some useful helper functions
(defn greet [name] 
  (str "Hello, " name "!"))

(defn square [x] 
  (* x x))

(defn cube [x] 
  (* x x x))

;; Welcome message
(print "Init file loaded successfully!")
(print "Try: (greet \"World\"), (square 5), (cube 3)")